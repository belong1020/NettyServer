<?xml version="1.0" encoding="UTF-8"?>
<iso8583-config>

	<!-- 长度全部都是字节数（由自己换算,LLVAR,LLLVAR,LLVAR_NUMERIC等变长数据域定义的长度无效） -->

	<!-- 统一头，每次不管请求还是响应都包含的共同内容 -->
	<header>
		<!-- 以下有标记*注释均为后添加条目 为iso8583报文模拟器（UPI）生成报文格式，与IsoField结构相同 16/11/2017 
			以下为各定长head域长度 文件头 0-53 bitmap 54-70（+16） -->
		<field id="MsgLength" type="NUMERIC" length="4" />				<!-- * 4字节 16进制数 需转ascii -->
		<field id="HeaderLength" type="CHAR" length="1"/>				<!-- * 1字节 16进制数 需转10进制 -->
		<field id="HeaderFlagandVersion" type="CHAR" length="1"/>		<!-- * 1字节 16进制数 需转ascii -->
		<field id="TotalMessageLength" type="NUMERIC" length="4" />		<!-- * 4字节 16进制数 BCD -->
		<field id="DestinationID" type="NUMERIC" length="8" />			<!-- * 8字节 16进制数 需转ascii -->
		<field id="Blank01" type="NUMERIC" length="3" />				<!-- * 3字节 16进制 value=20 blank值 -->
		<field id="SourceID" type="NUMERIC" length="8" />				<!-- * 8字节 16进制数 需转ascii -->
		<field id="Blank02" type="NUMERIC" length="3" />				<!-- * 3字节 16进制 value=20 blank值 -->
		<field id="ReservedforUse" type="CHAR" length="3" />			<!-- * 3字节 16进制数 BCD -->
		<field id="BatchNumber" type="CHAR" length="1" />				<!-- * 1字节 16进制数 BCD -->
		<field id="TransactionInformation" type="NUMERIC" length="8" />	<!-- * 8字节 16进制数 需转ascii -->
		<field id="UserInformation" type="CHAR" length="1" />			<!-- * 1字节 16进制数 BCD -->
		<field id="RejectCode" type="NUMERIC" length="5" />				<!-- * 5字节 16进制数 需转ascii -->
		<field id="mti" type="NUMERIC" length="4" />					<!-- * 4字节 16进制数 需转ascii -->
		<field id="BitMap" type="BITMAP" length="8" />					<!-- * 8/16字节 16进制数 需转ascii -->
	</header>

	<package>
		<!-- primary account number -->
		<field id="2" type="LLVAR_NUMERIC" length="19" />
		<!-- processing code -->
		<field id="3" type="NUMERIC" length="6" />
